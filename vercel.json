{
  "version": 2,
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/main.py"
    }
  ],
  "env": {
    "GITHUB_TOKEN": "@github_token",
    "SMTP_SENDER": "@smtp_sender", 
    "SMTP_PASSWORD": "@smtp_password"
  }
}
